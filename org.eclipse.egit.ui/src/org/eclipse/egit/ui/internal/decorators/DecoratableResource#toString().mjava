	@Override
	public String toString() {
		return getClass().getSimpleName() + "[" + getName() 				+ (isTracked() ? ", tracked" : "") 				+ (isIgnored() ? ", ignored" : "") 				+ (isDirty() ? ", dirty" : "") 				+ (hasConflicts() ? ", conflicts" : "") 				+ ", staged=" + getStagingState() 				+ "]"; 	}

