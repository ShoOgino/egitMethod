	@Override
	protected String getFilterString() {
		String filterString = super.getFilterString();
		if (!filterText.getText().equals(initialText)
				&& filterString.indexOf("*") != 0 				&& filterString.indexOf("?") != 0 				&& filterString.indexOf(".") != 0) {			filterString = "*" + filterString; 		}
		return filterString;
	}

