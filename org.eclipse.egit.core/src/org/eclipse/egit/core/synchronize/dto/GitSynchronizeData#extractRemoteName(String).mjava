	private String extractRemoteName(String rev) {
		if (rev.contains(R_REMOTES)) {
			String remote = rev.replaceAll(R_REMOTES, ""); 			return remote.substring(0, remote.indexOf("/")); 		} else {
			String name = BRANCH_NAME_PATTERN.matcher(rev).replaceAll(""); 			String remoteTracking = repo.getConfig().getString(CONFIG_BRANCH_SECTION,
					name, CONFIG_KEY_REMOTE);
			if (remoteTracking != null && remoteTracking.length() > 0)
				return remoteTracking;
			else
				return null;
		}
	}

