	@SuppressWarnings("unchecked")
	public static <V> V adapt(Object object, Class<V> target) {
		if (object == null)
			return null;
		if (target.isInstance(object))
			return (V) object;
		if (object instanceof IAdaptable)
			return Utils.getAdapter(((IAdaptable) object), target);
		return null;
	}

